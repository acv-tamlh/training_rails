<h1>Product#index</h1>
<p>Find me in app/views/product/index.html.erb</p>
<% @products.each do |product| %>
<h1>
  <%= product.title %>
</h1>
<%end %>

<table>
  <tr>
    <th>Product</th>
    <th>Price</th>
    <th>Published</th>
    <th>Category</th>
  </tr>
  <% @products.each do |product| %>
      <tr>
        <td><%= product.title%></td>
        <td><%= product.price%></td>
        <td><%= product.published%></td>
        <td><%= product.category.try(:title)%></td>
      </tr>
  <%end %>
</table>
