<h1>Products#new</h1>
<p>Find me in app/views/products/new.html.erb</p>
<%= semantic_form_for @product do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :price, min: 1 %>
  <%= f.input :country, as: :select %>
  <%= f.input :level %>
  <%= f.input :category %>
  <%= f.input :published %>
  <%= f.submit :submit %>
<% end %>
